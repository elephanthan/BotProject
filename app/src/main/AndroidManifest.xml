<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.worksmobile.android.botproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".view.Chat.ChatroomList.ChatroomListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".view.Chat.Chatroom.ChatroomActivity"
            android:parentActivityName=".view.Chat.ChatroomList.ChatroomListActivity" />
        <activity android:name=".view.Chat.NewChatting.NewChattingActivity" android:parentActivityName=".view.Chat.ChatroomList.ChatroomListActivity" />
        <activity android:name=".view.MySetting.MySettingActivity" android:parentActivityName=".view.Chat.ChatroomList.ChatroomListActivity" />

    </application>

</manifest>